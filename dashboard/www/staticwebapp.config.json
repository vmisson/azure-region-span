{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "*.{css,js,png,jpg,gif,ico,svg,json}"]
  },
  "globalHeaders": {
    "Cache-Control": "max-age=3600"
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "platform": {
    "apiRuntime": "node:20"
  }
}
